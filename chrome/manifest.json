{
  "name": "Українська Латинка",
  "description": "Виконує заміну українського кирилічного письма на веб сторінках латинкою",
  "version": "0.9",
  "permissions": [
    "tabs", "http://*/*", "https://*/*"
  ],
  "icons": {
      "128": "icons/icon128x128.png",
      "16": "icons/icon16x16.png",
      "48": "icons/icon48x48.png"
  },
  "browser_action": {
      "default_icon": {
          "19": "icons/icon19x19.png",
          "38": "icons/icon38x38.png"
      },
      "default_title": "Українська латинка",
      "default_popup": "popup.html"
  },
  "manifest_version": 2
}
